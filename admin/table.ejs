        <%- include ('header.ejs'); -%>
        <div id="page-wrapper" >
    		<div class="header"> 
                <ol class="breadcrumb">
    			  <li><a href="#">Home</a></li>
    			  <li><a href="#">Tables</a></li>
    			  <li class="active">Data</li>
    			</ol> 
    		</div>
		    <div id="page-inner"> 
            <div class="row">
                <div class="col-md-12">
                    <div class="panel panel-default">
                        <div class="panel-heading">
                                Advanced Tables
                                <div class="text-right">
                                    <a href="/table">New User</a>
                                </div>
                        </div>
                        <div class="panel-body">
                            <div class="table-responsive">
                                <table class="table table-striped table-bordered table-hover" id="dataTables-example">
                                    <thead>
                                        <tr>
                                            <th>User Name</th>
                                            <th>VRM</th>
                                            <th>Technician Name</th>
                                            <th>Start Date</th>
                                            <th>End Date</th>                                            
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <% print.forEach(function (user) { %>
                                            <tr class="odd gradeX">  
                                                <td><%= user.user_name %></td>  
                                                <td><%= user.vrm %></td>  
                                                <td><%= user.tech_name %></td>
                                                <td><%= user.start_date.toDateString() %></td>  
                                                <td><%= user.end_date.toDateString() %></td>
                                            </tr>                                   
                                         <% }); %> 
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <footer><p>All right reserved. Template by: <a href="https://github.com/vikashchaudhary007">Vikash Chaudhary</a></p></footer>
    </div>
    </div>
    <script src="http://localhost/myapp/admin/assets/js/jquery-1.10.2.js"></script>
    <script src="http://localhost/myapp/admin/assets/js/bootstrap.min.js"></script>
    <script src="http://localhost/myapp/admin/assets/js/jquery.metisMenu.js"></script>
    <script src="http://localhost/myapp/admin/assets/js/dataTables/jquery.dataTables.js"></script>
    <script src="http://localhost/myapp/admin/assets/js/dataTables/dataTables.bootstrap.js"></script>
        <script>
            $(document).ready(function () {
                $('#dataTables-example').dataTable();
            });
    </script>
    <script src="http://localhost/myapp/admin/assets/js/custom-scripts.js"></script>
</body>
</html>